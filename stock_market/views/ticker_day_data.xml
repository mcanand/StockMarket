<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="view_ticker_day_data_form" model="ir.ui.view">
        <field name="name">Ticker Day Data Form View</field>
        <field name="model">stock.ticker.day.data</field>
        <field name="arch" type="xml">
            <form string="Stock History">
                <sheet>
                    <group>
                        <group>
                            <field name="date_time"/>
                            <field name="open"/>
                            <field name="high"/>
                        </group>
                        <grouo>
                            <field name="volume"/>
                            <field name="close"/>
                            <field name="low"/>
                        </grouo>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    <record id="view_ticker_day_data_tree" model="ir.ui.view">
        <field name="name">Ticker Day Data tree View</field>
        <field name="model">stock.ticker.day.data</field>
        <field name="arch" type="xml">
            <form string="Day Data">
                <tree>
                    <field name="date_time"/>
                    <field name="open"/>
                    <field name="close"/>
                    <field name="high"/>
                    <field name="low"/>
                    <field name="volume"/>
                </tree>
            </form>
        </field>
    </record>
</odoo>